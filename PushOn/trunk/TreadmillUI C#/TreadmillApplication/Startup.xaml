	xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
	xmlns:pi="http://schemas.microsoft.com/prototyping/2010/interactivity"
	x:Class="TreadmillApplication.Startup"
	x:Name="Page"
	WindowTitle="Page"
	FlowDirection="LeftToRight"
